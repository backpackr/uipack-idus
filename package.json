{
  "name": "uipack-idus",
  "version": "1.3.0",
  "description": "UI Pack for idus.com",
  "main": "./dist/uipack.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/backpackr/uipack-idus.git"
  },
  "scripts": {
    "start": "parcel src/index.html --open --out-dir dev",
    "build": "parcel build src/js/uipack.js --no-source-maps",
    "deploy": "npm run build && npm publish",
    "docs": "rm -rf  docs && parcel build src/index.html --no-source-maps --out-dir docs --public-url '/uipack-idus/'",
    "lint": "eslint src",
    "webpack:build": "cross-env NODE_ENV=production webpack",
    "webpack:dev": "cross-env NODE_ENV=development webpack-dev-server"
  },
  "keywords": [
    "CSS Framework",
    "UI"
  ],
  "author": "https://github.com/7ylee",
  "homepage": "https://backpackr.github.io/uipack-idus/",
  "url": "https://github.com/backpackr/uipack-idus/issues",
  "email": "taeyouklee@gmail.com",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "last 2 version",
    "> 5%"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "sass": "^1.20.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  }
}
