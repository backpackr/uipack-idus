{
  "name": "uipack-idus",
  "version": "1.1.1",
  "description": "UI Pack for idus.com",
  "main": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/backpackr/uipack-idus.git"
  },
  "scripts": {
    "start": "parcel src/index.html --open --out-dir dev",
    "build": "npm run lint && parcel build src/js/uipack.js --no-source-maps",
    "docs": "rm -rf  docs && parcel build src/index.html --no-source-maps --out-dir docs --public-url '/uipack-idus/'",
    "lint": "eslint src",
    "browsersync": "npm start & node browsersync.js"
  },
  "keywords": [
    "CSS Framework",
    "UI"
  ],
  "author": "7ylee",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "browser-sync": "^2.24.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.9.3",
    "parcel": "^1.9.7",
    "prettier": "^1.14.2",
    "sass": "^1.13.0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
