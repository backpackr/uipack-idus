{
  "name": "uipack-idus",
  "version": "1.4.0",
  "description": "UI Pack for idus.com",
  "main": "./dist/uipack.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/backpackr/uipack-idus.git"
  },
  "scripts": {
    "start": "parcel src/index.html --open --out-dir dev",
    "build": "parcel build src/js/uipack.js --no-source-maps",
    "deploy": "npm run build && npm publish",
    "docs": "rm -rf  docs && parcel build src/index.html --no-source-maps --out-dir docs --public-url '/uipack-idus/'",
    "lint": "eslint src",
    "bs": "node browsersync.js"
  },
  "keywords": [
    "CSS Framework",
    "UI"
  ],
  "author": "https://github.com/7ylee",
  "homepage": "https://backpackr.github.io/uipack-idus/",
  "url": "https://github.com/backpackr/uipack-idus/issues",
  "email": "taeyouklee@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "browser-sync": "^2.26.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "parcel": "^2.8.2",
    "prettier": "^1.17.1",
    "sass": "^1.20.1"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  }
}
